<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.checkfx.epController">
   <children>
      <AnchorPane prefHeight="549.0" prefWidth="1035.0" style="-fx-background-color: #33D4FF;">
         <children>
            <MenuButton layoutX="936.0" layoutY="2.0" mnemonicParsing="false" text="Menu">
               <items>
                  <MenuItem fx:id="lm" mnemonicParsing="false" onAction="#Logout" text="Log out" />
                  <MenuItem fx:id="bm" mnemonicParsing="false" onAction="#Back" text="Back" />
               </items>
            </MenuButton>
            <Label fx:id="l1" layoutX="49.0" layoutY="46.0" text="Username" />
            <ComboBox fx:id="cmbbx" layoutX="302.0" layoutY="183.0" onAction="#editlabel" prefHeight="98.0" prefWidth="327.0" />
            <Label layoutX="412.0" layoutY="142.0" text="Evaluate Project">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Button fx:id="accept" layoutX="254.0" layoutY="462.0" mnemonicParsing="false" onAction="#accept" text="Accept" />
            <Button fx:id="reject" layoutX="596.0" layoutY="462.0" mnemonicParsing="false" onAction="#reject" text="Reject" />
            <TextArea fx:id="text" layoutX="304.0" layoutY="281.0" prefHeight="144.0" prefWidth="327.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
